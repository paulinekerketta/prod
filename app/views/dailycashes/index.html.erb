<p id="notice"><%= notice %></p>

<h1>Dailycashes</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Sales</th>
      <th>Discount</th>
      <th>Visa</th>
      <th>Cashpurch</th>
      <th>Onlineord</th>
      <th>Totdriverpay</th>
      <th>Banklodgement</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dailycashes.each do |dailycash| %>
      <tr>
        <td><%= dailycash.date %></td>
        <td><%= dailycash.sales %></td>
        <td><%= dailycash.discount %></td>
        <td><%= dailycash.visa %></td>
        <td><%= dailycash.cashpurch %></td>
        <td><%= dailycash.onlineord %></td>
        <td><%= dailycash.totdriverpay %></td>
        <td><%= dailycash.banklodgement %></td>
        <td><%= link_to 'Show', dailycash %></td>
        <td><%= link_to 'Edit', edit_dailycash_path(dailycash) %></td>
        <td><%= link_to 'Destroy', dailycash, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dailycash', new_dailycash_path %>
